{% extends 'base.html.twig' %}

{% block title %}TeamTermsAcceptance{% endblock %}

{% block body %}
    <h1>TeamTermsAcceptance</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ team_terms_acceptance.id }}</td>
            </tr>
            <tr>
                <th>Accepted</th>
                <td>{{ team_terms_acceptance.accepted ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>AcceptedAt</th>
                <td>{{ team_terms_acceptance.acceptedAt ? team_terms_acceptance.acceptedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>TermsVersion</th>
                <td>{{ team_terms_acceptance.termsVersion }}</td>
            </tr>
            <tr>
                <th>IpAdress</th>
                <td>{{ team_terms_acceptance.ipAdress }}</td>
            </tr>
            <tr>
                <th>UserAgent</th>
                <td>{{ team_terms_acceptance.userAgent }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_team_terms_acceptance_index') }}">back to list</a>

    <a href="{{ path('app_team_terms_acceptance_edit', {'id': team_terms_acceptance.id}) }}">edit</a>

    {{ include('team_terms_acceptance/_delete_form.html.twig') }}
{% endblock %}
