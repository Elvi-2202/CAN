{% extends 'base.html.twig' %}

{% block title %}TeamTermsAcceptance index{% endblock %}

{% block body %}
    <h1>TeamTermsAcceptance index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Accepted</th>
                <th>AcceptedAt</th>
                <th>TermsVersion</th>
                <th>IpAdress</th>
                <th>UserAgent</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for team_terms_acceptance in team_terms_acceptances %}
            <tr>
                <td>{{ team_terms_acceptance.id }}</td>
                <td>{{ team_terms_acceptance.accepted ? 'Yes' : 'No' }}</td>
                <td>{{ team_terms_acceptance.acceptedAt ? team_terms_acceptance.acceptedAt|date('Y-m-d H:i:s') : '' }}</td>
                <td>{{ team_terms_acceptance.termsVersion }}</td>
                <td>{{ team_terms_acceptance.ipAdress }}</td>
                <td>{{ team_terms_acceptance.userAgent }}</td>
                <td>
                    <a href="{{ path('app_team_terms_acceptance_show', {'id': team_terms_acceptance.id}) }}">show</a>
                    <a href="{{ path('app_team_terms_acceptance_edit', {'id': team_terms_acceptance.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="7">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_team_terms_acceptance_new') }}">Create new</a>
{% endblock %}
